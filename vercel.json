{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/client",
  "framework": "vue",
  "nodeVersion": "23.x",
  "env": {},
  "functions": {
    "api/**/*.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/ssr"
    }
  ]
}
